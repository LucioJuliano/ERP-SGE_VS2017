          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="ExtratoEstoqueTableAdapter" GeneratorDataComponentClassName="ExtratoEstoqueTableAdapter" Name="ExtratoEstoque" UserDataComponentName="ExtratoEstoqueTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="BD_ERP_SGE (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT T1.ID_ITEM,ISNULL(T8.SLDANTERIOR,0) AS SALDOANTERIOR,T1.Id_Produto, T7.Grupo, T1.DtMovim, T1.Qtde, T1.TpMov, T1.VlrUnitario, T2.Referencia, T2.Descricao, T3.Estoque, T3.Descricao AS TIPOMOVIMENTO, 
       T6.RazaoSocial, T5.Documento, T5.NumDocumento,F1.Fantasia
FROM ExtratoEstoque AS T1 
   LEFT OUTER JOIN Produtos AS T2 ON T2.Id_Produto = T1.Id_Produto 
   LEFT OUTER JOIN TabelasAux AS T3 ON T3.Chave = T1.TpMov 
   LEFT OUTER JOIN MvEstoqueItens AS T4 ON T4.Id_Item = T1.Id_Item 
   LEFT OUTER JOIN MvEstoque AS T5 ON T5.Id_Mov = T4.Id_Mov 
   LEFT OUTER JOIN Pessoas AS T6 ON T6.Id_Pessoa = T5.Id_Pessoa 
   LEFT OUTER JOIN GrupoProduto AS T7 ON T7.Id_Grupo = T2.Id_Grupo
   LEFT JOIN Empresa_Filial F1 ON (F1.Id_Banco=T5.Id_FilialOrigDest)
   LEFT OUTER JOIN SALDOESTOQUE T8 ON (T8.ID_PRODUTO=T1.ID_PRODUTO AND T8.ID_LANC=(SELECT TOP 1 ID_LANC FROM SALDOESTOQUE  SLD
                                                                              WHERE  SLD.ID_PRODUTO=T1.ID_PRODUTO
                                                                                 AND SLD.DATA &lt;=Convert(DateTime,'22/09/2009 00:00:00',103) 
                                                                               ORDER BY SLD.ID_PRODUTO,SLD.DATA DESC))
WHERE (T3.Campo = 'TPMVEST')
UNION 
SELECT T1.ID_ITEM,ISNULL(T8.SLDANTERIOR,0) AS SALDOANTERIOR,T1.Id_Produto, T7.Grupo, T1.DtMovim, T1.Qtde, T1.TpMov, T1.VlrUnitario, T2.Referencia, T2.Descricao, T3.Estoque, T3.Descricao AS TIPOMOVIMENTO, 
       T6.RazaoSocial, T5.Id_Venda AS DOCUMENTO, T5.NumDocumento,' ' AS Fantasia
FROM ExtratoEstoque AS T1 
   LEFT OUTER JOIN Produtos AS T2 ON T2.Id_Produto = T1.Id_Produto 
   LEFT OUTER JOIN TabelasAux AS T3 ON T3.Chave = T1.TpMov 
   LEFT OUTER JOIN MvVendaItens AS T4 ON T4.Id_Item = T1.Id_Item 
   LEFT OUTER JOIN MvVenda AS T5 ON T5.Id_Venda = T4.Id_Venda 
   LEFT OUTER JOIN Pessoas AS T6 ON T6.Id_Pessoa = T5.Id_Pessoa 
   LEFT OUTER JOIN GrupoProduto AS T7 ON T7.Id_Grupo = T2.Id_Grupo   
   LEFT OUTER JOIN SALDOESTOQUE T8 ON (T8.ID_PRODUTO=T1.ID_PRODUTO AND T8.ID_LANC=(SELECT TOP 1 ID_LANC FROM SALDOESTOQUE  SLD
                                                                              WHERE  SLD.ID_PRODUTO=T1.ID_PRODUTO
                                                                                 AND SLD.DATA &lt;=Convert(DateTime,'22/09/2009 00:00:00',103) 
                                                                               ORDER BY SLD.ID_PRODUTO,SLD.DATA DESC))
WHERE (T3.Campo = 'VENDA')
ORDER BY T1.DtMovim</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
            </MainSource>
