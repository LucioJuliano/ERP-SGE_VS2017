          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="VendasPrevistaTableAdapter" GeneratorDataComponentClassName="VendasPrevistaTableAdapter" Name="VendasPrevista" UserDataComponentName="VendasPrevistaTableAdapter">
