                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        T2.Id_Venda, T2.NumDocumento, T4.Descricao AS TIPOVENDA, T3.RazaoSocial, T2.PrevEntrega, T1.Id_Produto, T1.Qtde
FROM            MvVendaItens AS T1 LEFT OUTER JOIN
                         MvVenda AS T2 ON T2.Id_Venda = T1.Id_Venda LEFT OUTER JOIN
                         Pessoas AS T3 ON T3.Id_Pessoa = T2.Id_Pessoa LEFT OUTER JOIN
