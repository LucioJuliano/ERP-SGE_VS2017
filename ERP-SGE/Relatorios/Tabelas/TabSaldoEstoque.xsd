                         Produtos AS T5 ON T5.Id_Produto = T1.Id_Produto
WHERE        (T2.Status IN (1, 2)) AND (T1.TipoItem = 'S') AND (T2.Id_Entregador = 0) AND (T4.Estoque = 2)
ORDER BY T1.Id_Produto</CommandText>
